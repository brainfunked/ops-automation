---
dummy:
fetch_directory: ~/ceph-ansible-keys
ceph_repository_type: cdn
ceph_origin: repository
ceph_repository: rhcs
ceph_rhcs_version: 4
ceph_iscsi_config_dev: false
monitor_interface: {{ ceph_monitor_interface }}
public_network: {{ ceph_public_network }}
cluster_network: {{ ceph_cluster_network }}
ceph_docker_image: "rhceph/rhceph-4-rhel8"
ceph_docker_image_tag: "latest"
ceph_docker_registry: "registry.redhat.io"
ceph_docker_registry_auth: true
ceph_docker_registry_username: {{ ceph_docker_registry_username }}
ceph_docker_registry_password: {{ ceph_docker_registry_password }}
containerized_deployment: true
dashboard_admin_user: {{ ceph_gui_user }}
dashboard_admin_password: {{ ceph_gui_password }}
node_exporter_container_image: registry.redhat.io/openshift4/ose-prometheus-node-exporter:v4.1
grafana_admin_user: {{ ceph_gui_user }}
grafana_admin_password: {{ ceph_gui_password }}
grafana_container_image: registry.redhat.io/rhceph/rhceph-4-dashboard-rhel8:4
prometheus_container_image: registry.redhat.io/openshift4/ose-prometheus:4.1
alertmanager_container_image: registry.redhat.io/openshift4/ose-prometheus-alertmanager:4.1
